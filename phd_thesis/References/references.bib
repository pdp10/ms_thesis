	issn = "0264-6021",
	journal = "Biochemical Journal",
	month = mar,
	number = "Pt 2",
	pages = "361--71",
	title = "{Regulation of the ABC kinases by phosphorylation: protein kinase C as a paradigm}",
	url = "http://www.ncbi.nlm.nih.gov/pubmed/12495431",
	volume = "370",
	year = "2003"
}

