	doi = "10.1038/nrg2633",
	journal = "Nature Reviews Genetics",
	mesh = "Algorithms; Animals; Genetic Techniques; Genomics; Humans; Mass Spectrometry; Models, Biological; Proteomics; Systems Biology",
	month = "Sep",
	number = "9",
