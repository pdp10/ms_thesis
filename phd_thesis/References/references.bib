}

@article{Polak2009,
	abstract = "Mammalian Target of Rapamycin (mTOR) is a highly conserved protein kinase that functions as part of two distinct multiprotein complexes to regulate growth and metabolism. This review describes the most important recent advances in the mTOR signaling field. In addition, we provide an overview on the functions of mTOR in different organs, with a special focus on the role of mTOR in whole body energy metabolism.",
	affiliation = "Biozentrum, University of Basel, Basel, Switzerland.",
	author = "Polak, Pazit and Hall, Michael N",
	issn = "1879-0410",
	journal = "Current Opinion in Cell Biology",
